{
  "name": "@angular-buch/styles",
  "version": "0.0.4",
  "description": "A global stylesheet to be used for the application \"BookManager\"",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/angular-buch/styles.git"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "main": "dist/styles.css",
  "sass": "src/styles.scss",
  "style": "dist/styles.css",
  "scripts": {
    "build": "sass --load-path=node_modules/@picocss/pico/scss/ src/styles.scss dist/styles.css",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "BookManager",
    "Angular",
    "Buch",
    "Angular",
    "CSS",
    "SCSS",
    "Stylesheet"
  ],
  "author": "Angular Buch Team<team@angular-buch.com> (https://angular-buch.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular-buch/styles/issues"
  },
  "homepage": "https://github.com/angular-buchstyles#readme",
  "dependencies": {
    "@picocss/pico": "2.1.1",
    "sass": "1.83.4"
  }
}
